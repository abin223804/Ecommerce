            <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

            <script>
                const ctx = document.getElementById('myChart');
                const name = document.getElementById('pname');
                const scategory=document.getElementById('scategory')
                const count = document.getElementById('pcount');
                const pnames = name.value.split(',');
                const pcounts = count.value.split(',');


                new Chart(ctx, {
                    type: 'bar',
                    data: {
                        labels: scategory,
                        datasets: [{
                            label: 'sales',
                            data: pcounts,
                            borderWidth: 1
                        }]
                    },
                    options: {
                        scales: {
                            y: {
                                beginAtZero: true
                            }
                        }
                    }
                });


                const ctx1 = document.getElementById('myPie');
                const name1 = document.getElementById('category');
                const count1 = document.getElementById('count');
                const category = name1.value.split(',');
                const countPie = count1.value.split(',');


                new Chart(ctx1, {
                    type: 'pie',
                    data: {
                        labels: category,
                        datasets: [{
                            label: 'sales',
                            data: countPie,
                            borderWidth: 1
                        }]
                    },
                    // options: {
