<style>
  .site-logo {
  margin-left: 500px;
}

.site-logo h1 {
  font-size: 36px;
  font-weight: bold;
  letter-spacing: 2px;
  text-shadow: 2px 2px #333;
  transition: transform 0.5s;
}

.site-logo h1:hover {
  transform: rotateY(180deg);
}

</style>
<script>
  // Select the h1 element with the data-3d-text attribute
const h1 = document.querySelector('h1[data-3d-text]');

// Split the text into individual characters
const characters = h1.innerText.split('');

// Wrap each character in a span tag with a unique class
const wrappedCharacters = characters.map((char, index) => {
  return `<span class="char-${index + 1}">${char}</span>`;
});

// Replace the original text with the wrapped characters
h1.innerHTML = wrappedCharacters.join('');

// Add a mousemove event listener to the document
document.addEventListener('mousemove', (e) => {
  // Calculate the rotation angle based on the mouse position
  const rotateY = (window.innerWidth / 2 - e.pageX) / 10;
  const rotateX = (window.innerHeight / 2 - e.pageY) / 10;

  // Apply the rotation to each character
  characters.forEach((char, index) => {
    const charElement = document.querySelector(`.char-${index + 1}`);
    charElement.style.transform = `rotateY(${rotateY}deg) rotateX(${rotateX}deg)`;
  });
});

</script>
