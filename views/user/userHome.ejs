                  <div class="arrow">
                    <span class="prev"></span>
                    <span class="next"></span>
                  </div>
                </div>
              </div>
              
              <script>
                const images = ['/admin/multer/img/<%=banners.bannerImage[0] %>', '/admin/multer/img/<%=banners.bannerImage[1] %>', '/admin/multer/img/<%=banners.bannerImage[2] %>'];
                let currentIndex = 0;
                const imgElement = document.querySelector('.single-exclusive-slider img');
              
                function showNextImage() {
                  currentIndex++;
                  if (currentIndex === images.length) {
                    currentIndex = 0;
                  }
                  imgElement.src = images[currentIndex];
                }
              
                function showPreviousImage() {
                  currentIndex--;
                  if (currentIndex < 0) {
                    currentIndex = images.length - 1;
                  }
                  imgElement.src = images[currentIndex];
                }
              
                document.querySelector('.arrow .prev').addEventListener('click', showPreviousImage);
                document.querySelector('.arrow .next').addEventListener('click', showNextImage);
              
                setInterval(showNextImage, 5000); // change 5000 to the interval time you want in milliseconds
              </script>
              
